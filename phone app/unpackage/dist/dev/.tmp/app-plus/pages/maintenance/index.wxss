
.content{
	height:100%;
	width:100%;
	background-color: #FFFFFF;
}
.cart-none{
	height:100%;
	width:100%;
	background-color: #FFFFFF;
	padding: 200rpx 0;
}
.cart-none-img{
	width: 252rpx;
	height: 228rpx;
	margin-bottom: 40rpx;
}
.cart-none-t{
	color: #666;
	font-size: 28rpx;
}
.cart-none-m{
	color: #666;
	font-size: 28rpx;
	margin-bottom: 40rpx;
}
.cart-bottom {
	
	
	bottom: 0;
	
	z-index: 99;
	height: 90rpx;
	width: 100%;
	background-color: #fff;
	position: fixed;
	
	overflow: hidden;
	/* box-shadow: 0 0 20upx #ccc; */
}
.cart-bottom .cart-checkbox-c {
	color: #333;
	font-size: 30rpx;
}
.cart-checkbox{
	position: relative;
	height: 100%;
}
.cart-checkbox-c{
	display: inline-block;
	position: absolute;
	top: 50%;
	left: 26rpx;
	-webkit-transform: translateY(-50%);
	    -ms-transform: translateY(-50%);
	        transform: translateY(-50%);
	z-index: 99;
}
.cart-bottom-right {
	height: 90rpx;
	float: right;
	overflow: hidden;
}
